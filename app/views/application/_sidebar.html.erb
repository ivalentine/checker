<aside class="sidebar col-md-3">
  <div id="home">
    <%= link_to "Home", root_path %>
  </div>
  <div id="lists">
    <ul>
      <% current_user.lists.each do |list| %>
      <li><%= link_to list.name, list_path(list.id) %></li>
      <% end %>
    </ul>
  </div>
  <div id="create-list">
    <%= link_to "Create list", new_list_path %>
  </div>
</aside>
